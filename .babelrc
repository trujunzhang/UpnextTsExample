{
  "presets": [
    "next/babel",
    "@zeit/next-typescript/babel"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": [
          "."
        ],
        "alias": {
          "@src": "./src",
          "@config": "./config",
          "@assets": "./static"
        },
        "cwd": "babelrc"
      }
    ]
  ]
}
